{
    "name": "@galaxybase/galaxyvis",
    "version": "1.0.2",
    "description": "A High Performance Graph Visualization Framework in JavaScript",
    "homepage": "https://galaxyvis.com/",
    "main": "index.js",
    "author": "galaxybase",
    "license": "Apache License 2.0",
    "scripts": {
        "examples": "webpack serve --config ./examples/webpack.config.js --progress",
        "build": "webpack --progress && webpack --progress --mode production",
        "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,less,md}\"",
        "transpile": "tsc -p ./tsconfig.transpile.json -d && replace '\\.glsl' '.glsl.js' renderers -r && webpack --config ./webpack.programs.config.js",
        "clean": "rimraf captors classes initial layouts renderers types utils index.js index.d.ts galaxyvis.js galaxyvis.d.ts",
        "postpublish": "npm run clean",
        "prepublishOnly": "npm run clean && npm run transpile"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.173",
        "dat.gui": "^0.7.7",
        "eslint": "^8.10.0",
        "html-webpack-plugin": "^5.3.1",
        "prettier": "^2.5.1",
        "raw-loader": "^4.0.2",
        "replace": "^1.2.1",
        "rimraf": "^3.0.2",
        "ts-loader": "^9.2.5",
        "typescript": "^4.4.2",
        "webpack": "^5.37.1",
        "webpack-cli": "^4.7.0",
        "webpack-dev-server": "^3.11.2"
    },
    "dependencies": {
        "dagre": "^0.8.5",
        "gl-matrix": "^3.3.0",
        "lodash": "^4.17.21",
        "polyline-normals": "^2.0.0",
        "worker-loader": "^3.0.8"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/galaxybase/GalaxyVis"
    },
    "keywords": [
        "visualization",
        "javascript",
        "graph",
        "graph-visualization",
        "graph-analysis",
        "webgl",
        "typescript",
        "canvas"
    ]
}
